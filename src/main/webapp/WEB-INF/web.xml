	<error-page> 
		<error-code>401</error-code>
		<location>/error/general-error.html</location>
	</error-page>
	<error-page> 
		<error-code>403</error-code>
		<location>/error/general-error-403.html</location>
	</error-page>
	<error-page>
		<!-- Missing resource -->
		<error-code>404</error-code>
		<location>/error/Error404.html</location>
	</error-page>
	<error-page>
		<!-- Uncaught exception -->
		<error-code>500</error-code>
		<location>/error/general-error-500.html</location>
	</error-page>
	<error-page>
		<!-- Unsupported servlet method -->
		<error-code>503</error-code>
		<location>/error/general-error-503.html</location>
	</error-page>

